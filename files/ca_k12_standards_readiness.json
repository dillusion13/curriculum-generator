            "reading_informational": {
              "description": "Main idea, text structure, author's purpose, multiple sources",
              "progression": "Main idea with details > Text structure > Author's evidence and reasoning"
            },
            "writing": {
              "description": "Opinion/argument, informative/explanatory, narrative with development",
              "progression": "Introduce topic/state opinion > Organize with logic > Clear, coherent writing"
            },
            "speaking_listening": {
              "description": "Discussion preparation, summarizing, formal presentations",
              "focus": "Collaborative discussions, report on topics, recount experiences"
            },
            "language": {
              "description": "Complex sentences, word relationships, academic vocabulary",
              "focus": "Conventions, context clues, affixes/roots, word nuances"
            }
          },
          "readiness_indicators": {
            "below": {
              "signs": ["Reads 1-2 grades below", "Struggles with multi-syllable words", "Cannot identify main idea", "Limited inference", "Disorganized writing", "Limited vocabulary"],
              "supports": ["Fluency practice", "Comprehension strategies", "Graphic organizers", "Word study/morphology", "Text-dependent questioning"]
            },
            "at": {
              "signs": ["Reads grade-level texts", "Identifies theme/main idea", "Makes inferences", "Organized writing", "Grade-level vocabulary", "Meaningful discussions"]
            },
            "above": {
              "signs": ["Complex text comprehension", "Analyzes craft/structure", "Cross-text connections", "Voice and complexity in writing", "Advanced vocabulary", "Leads discussions"],
              "extensions": ["Book clubs", "Primary source research", "Literary analysis", "Debate", "Cross-curricular connections"]
            }
          }
        },
        "6-8": {
          "focus": "Literary analysis; argument and evidence; discipline-specific literacy",
          "key_domains": {
            "reading_literature": {
              "description": "Theme development, figurative language, point of view, comparing versions",
              "key_standards": ["Cite textual evidence", "Analyze theme development", "Analyze story elements", "Figurative/connotative meaning", "Point of view development"]
            },
            "reading_informational": {
              "description": "Central idea, text structure, argument evaluation, source comparison",
              "key_standards": ["Cite evidence", "Central idea analysis", "Text structure", "Author purpose", "Evaluate argument/claims"]
            },
            "writing": {
              "description": "Argument, informative/explanatory, narrative; research",
              "key_standards": ["Arguments with claims/evidence", "Informative texts", "Narratives with technique", "Short research projects"]
            },
            "literacy_disciplines": {
              "description": "Reading/writing in History/Social Science and Science/Technical subjects",
              "focus": "Discipline-specific reading strategies and writing expectations"
            }
          },
          "readiness_indicators": {
            "below": {
              "signs": ["Reads 2+ grades below", "Cannot cite evidence", "Struggles with central idea", "Cannot analyze craft", "Writing lacks claims/evidence", "Limited academic vocabulary"],
              "supports": ["Close reading scaffolds", "Writing templates (RACE, TREES)", "Vocabulary with morphology", "Audio support", "Collaborative annotation"]
            },
            "at": {
              "signs": ["Grade-level comprehension", "Cites relevant evidence", "Analyzes theme/central idea", "Understands author craft", "Arguments with claims/evidence", "Appropriate vocabulary"]
            },
            "above": {
              "signs": ["Complex text analysis", "Sophisticated inferences", "Evaluates reasoning quality", "Nuanced arguments", "Precise vocabulary", "Critical engagement"],
              "extensions": ["Socratic seminars", "Primary source research", "Literary criticism", "Debate", "Peer mentorship"]
            }
          }
        },
        "9-12": {
          "focus": "College and career readiness; advanced analysis; research",
          "key_domains": {
            "reading_literature": {
              "description": "Complex theme analysis, multiple interpretations, foundational texts",
              "key_standards": ["Strong/thorough evidence", "Theme over course of text", "Complex character analysis", "Cumulative impact of word choice", "Cultural experience analysis"]
            },
            "reading_informational": {
              "description": "Analysis of seminal documents, rhetoric, multiple sources",
              "key_standards": ["Thorough evidence", "Central idea development", "Claim development/refinement", "Rhetoric analysis", "Argument evaluation"]
            },
            "writing": {
              "description": "Argument, research, narrative; style and voice",
              "key_standards": ["Valid reasoning/relevant evidence", "Complex ideas conveyance", "Clear coherent writing", "Sustained research"]
            }
          },
          "readiness_indicators": {
            "below": {
              "signs": ["Cannot read grade-level texts", "Cannot analyze complex themes", "Cannot construct arguments", "Unaware of rhetoric", "Incoherent writing", "Limited vocabulary"],
              "supports": ["Reading intervention", "Sentence/paragraph instruction", "Explicit vocabulary", "Annotation protocols", "Argument organizers"]
            },
            "at": {
              "signs": ["Complex text comprehension", "Analyzes theme/structure/rhetoric", "Coherent arguments", "Synthesizes sources", "College-ready writing", "Appropriate vocabulary"]
            },
            "above": {
              "signs": ["Highly complex texts", "Sophisticated analysis", "Distinctive voice/style", "College-level research", "Exceptional thinking", "AP/honors ready"],
              "extensions": ["AP coursework", "Dual enrollment", "Independent study", "Publication", "Academic competitions"]
            }
          }
        }
      }
    },
