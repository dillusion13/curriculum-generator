  "readiness_levels": {
    "description": "Based on Smarter Balanced/CAASPP Achievement Level Descriptors",
    "levels": {
      "level_1_below": {
        "label": "Standard Not Met",
        "description": "Student does not consistently demonstrate grade-level knowledge and skills",
        "action": "Intensive intervention needed; focus on prerequisite skills"
      },
      "level_2_approaching": {
        "label": "Standard Nearly Met",
        "description": "Student demonstrates foundational grade-level knowledge and skills with limited range of complexity",
        "action": "Strategic support needed; scaffolded instruction with grade-level content"
      },
      "level_3_at": {
        "label": "Standard Met",
        "description": "Student consistently demonstrates proficient grade-level knowledge and skills with broad range of complexity",
        "action": "On track; continue grade-level instruction with enrichment opportunities"
      },
      "level_4_above": {
        "label": "Standard Exceeded",
        "description": "Student consistently demonstrates advanced grade-level knowledge and skills with deep understanding and full range of complexity",
        "action": "Extension activities; accelerated content; leadership opportunities"
      }
    }
  },
  "subjects": {
    "ela": {
      "full_name": "English Language Arts & Literacy",
      "standards_source": "CA Common Core State Standards for ELA/Literacy",
      "strands": ["Reading", "Writing", "Speaking and Listening", "Language"],
      "anchor_standards": "College and Career Readiness Anchor Standards",
      "grade_bands": {
        "K-2": {
          "focus": "Foundational literacy skills development",
          "key_domains": {
            "reading_foundational": {
              "description": "Print concepts, phonological awareness, phonics, fluency",
              "K": ["RF.K.1-4: Print concepts, phonemic awareness, phonics basics, read emergent texts"],
              "1": ["RF.1.1-4: Advanced phonics, decode one-syllable words, read with fluency"],
              "2": ["RF.2.3-4: Decode multi-syllable words, prefixes/suffixes, read fluently"]
            },
            "reading_literature": {
              "description": "Key ideas, craft and structure, integration of knowledge",
              "progression": "Ask questions > Retell with key details > Describe characters/settings/events"
            },
            "reading_informational": {
              "description": "Comprehension of nonfiction texts",
              "progression": "Identify main topic > Key details > Connections between information"
            },
            "writing": {
              "description": "Opinion, informative/explanatory, narrative writing",
              "progression": "Draw/dictate/write > Simple sentences > Organized paragraphs"
            },
            "speaking_listening": {
              "description": "Collaborative discussions, presentation",
              "focus": "Participate in conversations, confirm understanding, describe/tell stories"
            },
            "language": {
              "description": "Grammar, vocabulary acquisition",
              "focus": "Conventions, capitalization/punctuation, word meanings"
            }
          },
          "readiness_indicators": {
            "below": {
              "signs": ["Cannot recognize letters/sounds", "No phonemic awareness", "Cannot decode CVC words", "Limited sight words", "Cannot retell stories", "Writing limited to scribbles"],
              "supports": ["Intensive phonics", "Small group instruction", "Multi-sensory approaches", "Decodable texts", "Explicit vocabulary"]
            },
            "at": {
              "signs": ["Knows letter names/sounds", "Decodes grade-level words", "Recognizes sight words", "Reads with comprehension", "Writes simple sentences", "Participates in discussions"]
            },
            "above": {
              "signs": ["Reads above grade level", "Strong vocabulary", "Makes inferences", "Writes with detail", "Asks higher-order questions"],
              "extensions": ["Independent reading", "Literature circles", "Research projects", "Creative writing", "Peer tutoring"]
            }
          }
        },
        "3-5": {
          "focus": "Reading to learn; developing analytical skills",
          "key_domains": {
            "reading_literature": {
              "description": "Theme, character development, point of view, text comparison",
              "progression": "Determine message/moral > Determine theme > Compare/contrast across texts"
